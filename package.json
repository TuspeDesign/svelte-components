{
  "name": "@tuspe/components",
  "version": "1.9.7",
  "description": "A reusable SvelteKit component library for form elements, breadcrumbs, prices and images.",
  "keywords": [
    "svelteKit",
    "components",
    "cache",
    "modal",
    "breadcrumb",
    "form",
    "numbers",
    "image",
    "input",
    "checkbox",
    "select",
    "preventDefault",
    "validation"
  ],
  "homepage": "https://github.com/TuspeDesign/svelte-components",
  "license": "MIT",
  "author": {
    "name": "Timo Anttila",
    "url": "https://timoanttila.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TuspeDesign/svelte-components.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "prepack": "svelte-kit sync && svelte-package",
    "publint": "publint .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "publish": "npm publish --access public"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": false,
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.17.2",
    "svelte": "^5.20.2"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/enhanced-img": "^0.9.0",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/package": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.15",
    "svelte": "^5.43.14",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.4"
  }
}